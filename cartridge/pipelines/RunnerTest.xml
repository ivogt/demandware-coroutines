        </branch>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <jump-node start-name-ref="Delegate-Run"/>
        <node-display x="0" y="1"/>
      </node>
    </segment>
  </branch>
